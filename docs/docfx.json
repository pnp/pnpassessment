{
  "build": {
    "content": [
      {
        "files": [
          "using-the-assessment-tool/**.md",
          "using-the-assessment-tool/toc.yml",
          "sharepoint-syntex/**.md",
          "sharepoint-syntex/toc.yml",
          "workflow/**.md",
          "workflow/toc.yml",
          "contributing/**.md",
          "contributing/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "templates/material"
    ],
    "xrefService": [
      "https://xref.docs.microsoft.com/query?uid={uid}"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false,
    "globalMetadata": {
      "_appTitle": "Microsoft 365 Assessment Tool",
      "_appFooter": "<span>Microsoft 365 Assessment Tool<br>Generated by <strong><a href='https://dotnet.github.io/docfx' alt='Doc FX Website'>DocFX</a></strong> with <strong><a href='https://ovasquez.github.io/docfx-material' alt='DocFx Material Theme'>Material UI</a></strong></span>",
      "_appFaviconPath": "https://c.s-microsoft.com/favicon.ico?v2",
      "_appLogoPath": "",
      "_description":"The Microsoft 365 SharePoint Assessment tool is enabling various deprecation and adoption scenarios by enabling you to assess your tenant.",
      "_enableSearch": true,
      "_enableNewTab": true,
      "_disableBreadcrumb" : true
    }
  }
}
